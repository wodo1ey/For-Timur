#include <iostream>
#include <iomanip>

using namespace std;

void main()
{
	setlocale(LC_ALL, "Russian");
	cout << setw(10) << left << "Рубль";
	cout << setw(10) << "Доллар";
	cout << setw(10) << "Евро" << endl;
	for (int i = 1; i <= 100; i++) cout << setw(7) << i << setw(10) << i * 60.36 << setw(10) << i * 62.41 << endl;
}
